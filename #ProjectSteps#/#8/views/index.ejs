<h2 class="page-header">Recently Added</h2>
<div class="book-row">
    <% books.forEach(book => { %>
        <a class="book-and-title" href="/books/<%= book.id %>">
            <img height="150" width="150" src="<%= book.coverImagePath %>" alt="">
            <span><%= book.title %></span>
        </a>
    <% }) %>
</div>